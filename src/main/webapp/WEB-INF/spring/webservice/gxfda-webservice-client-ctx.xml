<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:jaxws="http://cxf.apache.org/jaxws"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd"
>

    <jaxws:client
        id="ITransferOAServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.ITransferOAService"
        address="${nnccoa.webservice.url}"
    >
    </jaxws:client>

    <jaxws:client
        id="ITransferAipCaseServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.aipcasetransfer.AipCaseWebService"
        address="${nncccase.webservice.url}"
    >
    </jaxws:client>

    <jaxws:client
        id="ITransferVoiceServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.IVoiceWebService"
        address="${nnccvoice.webservice.url}"
    >
    </jaxws:client>

    <jaxws:client
        id="ITransferEmServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.ITransferEmService"
        address="${nnccem.webservice.url}"
    >
    </jaxws:client>
    <jaxws:client
        id="ITransferSuperviseServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.SuperviseWebService"
        address="${nnccapp.webservice.url}"
    >
    </jaxws:client>
    <jaxws:client
        id="ITransferDocexServiceClientFactory"
        serviceClass="cn.com.chaochuang.webservice.server.ITransferDocexService"
        address="${nnccdocex.webservice.url}"
    >
    </jaxws:client>
</beans>
