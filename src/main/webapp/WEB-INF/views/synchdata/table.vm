#set($bodyattr="class='easyui-layout' style='border:0px'")

<div class="cc-tableWrap" data-options="region:'center',border:false" style="padding:5px;">
<table id="dg" style="width:100%" data-options="fit:true,rownumbers:true,singleSelect:true,pagination:true,toolbar:'#tb',url:'synchdata/list.json',pageSize:20,pageList:[20, 30, 50]">
    <thead>
    <tr>
        <th data-options="field:'synchDataFlag',width:150">同步数据来源</th>
        <th data-options="field:'needSynch',width:150">需同步记录数</th>
        <th data-options="field:'finishSynch',width:150">已同步记录数</th>
        <th data-options="field:'beginTime',width:80">开始时间</th>
        <th data-options="field:'finishTime',width:80">结束时间</th>
        <th data-options="field:'status',width:80" >状态</th>
        <th data-options="field:'memo',width:500">同步描述</th>
    </tr>
    </thead>
</table>
</div>
